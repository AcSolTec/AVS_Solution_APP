                                                <div class="controls input-append date form_date" id="dtPickerIssue" data-date="" data-date-format="dd/mm/yy" data-link-field="dtp_issue" data-link-format="dd/mm/yy">
                                                    <input class="form-control" size="16" type="text" value="" readonly>
                                                    <span class="add-on"><i class="icon-remove"></i></span>
                                                    <span class="add-on"><i class="icon-th"></i></span>
                                                </div>
                                                <input type="hidden" id="dtp_issue" value="" /><br />
                                            </div>
                                            <div class="col-6 col-sm-3 mt-4 mt-sm-0">
                                                <label class="control-label">Date of Expiry</label>
                                                <div class="controls input-append date form_date" id="dtPickerExpiry" data-date="" data-date-format="dd/mm/yy" data-link-field="dtp_Expiry" data-link-format="dd/mm/yy">
